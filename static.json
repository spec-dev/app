{
    "root": "build/",
    "https_only": true,
    "routes": {
        "/favicon.png": "/favicon.png",
        "/fonts/*": "/fonts/",
        "/static/*": "/static/",
        "/**": "/index.html"
    },
    "headers": {
        "/": {
            "Cache-Control": "no-store, no-cache"
        },
        "/fonts/**": {
            "Cache-Control": "public, max-age=31536000"
        },
        "/static/**": {
            "Cache-Control": "public, max-age=31536000"
        }
    }
}