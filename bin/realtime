#!/bin/bash

docker run \
  -e DB_HOST='docker.for.mac.host.internal' \
  -e DB_NAME='garden' \
  -e DB_USER='benwhittle' \
  -e DB_PASSWORD='postgres' \
  -e DB_PORT=5432 \
  -e PORT=4000 \
  -e DB_SSL='false' \
  -e SECURE_CHANNELS='false' \
  -e JWT_SECRET='asdf' \
  -p 4000:4000 \
  supabase/realtime